{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "LogFilePath": "logs/DataFixter.log",
    "ConsoleOutput": true,
    "FileOutput": true,
    "RetentionDays": 30,
    "MaxFileSizeMB": 10
  },
  "DataProcessing": {
    "DefaultValidationColumns": [0, 1],
    "DefaultValues": {
      "0": "默认名称",
      "1": "0",
      "2": "默认描述",
      "3": "默认分类"
    },
    "BatchSize": 1000,
    "MaxConcurrentOperations": 4
  },
  "Excel": {
    "SupportedFormats": [".xlsx", ".xls"],
    "MaxFileSizeMB": 100,
    "AutoBackup": true,
    "BackupDirectory": "backups"
  }
}
