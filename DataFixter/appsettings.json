{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/DataFixter.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
          "retainedFileCountLimit": 30,
          "fileSizeLimitBytes": 10485760
        }
      }
    ]
  },
  "DataProcessing": {
    "DefaultValidationColumns": [0, 1],
    "DefaultValues": {
      "0": "默认名称",
      "1": "0",
      "2": "默认描述",
      "3": "默认分类"
    },
    "BatchSize": 1000,
    "MaxConcurrentOperations": 4
  },
  "Excel": {
    "SupportedFormats": [".xlsx", ".xls"],
    "MaxFileSizeMB": 100,
    "AutoBackup": true,
    "BackupDirectory": "backups"
  },
  "CorrectionOptions": {
    "CumulativeTolerance": 0.001,
    "CriticalThreshold": 1.0,
    "ErrorThreshold": 0.3,
    "MinValueThreshold": 0.001,
    "MaxCurrentPeriodValue": 1.0,
    "MaxCumulativeValue": 4.0,
    "MileageTolerance": 0.01,
    "MaxTimeInterval": 30.0,
    "EnableMinimalModification": true
  },
  "ValidationOptions": {
    "CumulativeTolerance": 0.01,
    "CriticalThreshold": 1.0,
    "ErrorThreshold": 0.3,
    "MinValueThreshold": 0.01,
    "MaxCurrentPeriodValue": 1.0,
    "MaxCumulativeValue": 4.0,
    "MileageTolerance": 0.01,
    "MaxTimeInterval": 30.0,
    "MaxProcessingTimeMinutes": 30,
    "BatchSize": 50,
    "EnableMemoryCleanup": true,
    "MemoryCleanupFrequency": 200,
    "MaxDegreeOfParallelism": 0
  },
  "OutputOptions": {
    "PreserveOriginalFormat": true,
    "AddCorrectionMarks": true,
    "OutputEncoding": "UTF-8"
  }
}
